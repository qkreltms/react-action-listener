{
  "name": "redux-action-listener-hook",
  "version": "1.0.2",
  "description": "Redux middleware hook which allows listening action and handling of dispatched actions",
  "main": "lib",
  "files": [
    "lib",
    "src"
  ],
  "directories": {
    "lib": "src",
    "example": "",
    "test": "test"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "npm run type-check -- --watch",
    "clean": "rimraf lib coverage",
    "build:p": "tsc --declarationDir lib --emitDeclarationOnly",
    "build": "babel src --out-dir lib --extensions \".ts,.tsx,js,jsx\" && yarn build:p",
    "test": "jest --watch",
    "test:c": "jest --coverage",
    "lint": "eslint --ext js,ts,jsx,tsx src test",
    "lint:p": "prettier --write \"{src,test}/**/*.{js,ts}\" \"**/*.md\""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn lint:p"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qkreltms/redux-action-listener-hook"
  },
  "keywords": [
    "redux",
    "action-middleware",
    "listeners",
    "listener-middleware",
    "middleware",
    "redux-middleware",
    "redux-action-listener-hook",
    "redux-hook"
  ],
  "author": {
    "name": "Jeong Hoon Park",
    "email": "junghooncentralpark@gmail.com",
    "url": "https://github.com/qkreltms/"
  },
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qkreltms/redux-action-listener-hook/issues"
  },
  "homepage": "https://github.com/qkreltms/redux-action-listener-hook#readme",
  "dependencies": {
    "@types/react": "^17.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "16.9.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  }
}
