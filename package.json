{
  "name": "use-redux-action-listener",
  "version": "0.1",
  "description": "Redux middleware hook which allows listening in on and handling of dispatched actions",
  "main": "lib/listenerMiddleware.js",
  "files": [
    "lib",
    "src"
  ],
  "directories": {
    "lib": "src",
    "example": "",
    "test": "test"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "npm run type-check -- --watch",
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib --extensions \".ts\"",
    "test": "jest",
    "lint": "eslint src/** --fix",
    "lint:p": "prettier --write .\\src\\"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn lint:p"
    ],
    "test/**/*.{ts,tsx,js,jsx}": [
      "yarn test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qkreltms/redux-listeners"
  },
  "keywords": [
    "redux",
    "action-middleware",
    "listeners",
    "listener-middleware",
    "middleware",
    "redux-middleware"
  ],
  "author": {
    "name": "Jeong Hoon Park",
    "email": "junghooncentralpark@gmail.com",
    "url": "https://github.com/qkreltms/"
  },
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qkreltms/redux-listeners/issues"
  },
  "homepage": "https://github.com/qkreltms/redux-listeners#readme",
  "dependencies": {
    "@types/jest": "^26.0.19",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "tape": "^5.0.1",
    "typescript": "^4.1.3"
  }
}
